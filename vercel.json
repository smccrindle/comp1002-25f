{
  "functions": {
    "**/*.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    {
      "src": "^/(.*)/?includes/(.*)$",
      "status": 404
    },
    {
      "src": "^/\\.git/(.*)$",
      "status": 404
    },
    {
      "src": "^/(.*)\\.env$",
      "status": 404
    },
    {
      "src": "^/(.*)\\.php$",
      "dest": "/api/_run.php"
    },
    {
      "src": "^/(.*)/$",
      "dest": "/api/_run.php"
    },
    {
      "src": "^/([^\\.]+)$",
      "dest": "/api/_run.php"
    },
    {
      "handle": "filesystem"
    }
  ]
}